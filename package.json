{
  "name": "@specialblend/with-eslint",
  "version": "0.0.0",
  "description": "quickly setup a project with eslint",
  "main": "with-eslint.sh",
  "bin": {
    "with-eslint": "with-eslint",
    "with-eslint-typescript": "with-eslint-typescript"
  },
  "scripts": {
    "test": "true",
    "build": "cat common.sh with-eslint.sh > with-eslint && cat common.sh with-eslint-typescript.sh > with-eslint-typescript",
    "preinstall": "npm run build",
    "postinstall": "chmod +x with-eslint with-eslint-typescript"
  },
  "files": [
    "common.sh",
    "with-eslint.sh",
    "with-eslint-typescript.sh"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/specialblend/with-eslint.git"
  },
  "keywords": [
    "eslint",
    "generator",
    "cli"
  ],
  "author": "cj@specialblend.net",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/specialblend/with-eslint/issues"
  },
  "homepage": "https://github.com/specialblend/with-eslint#readme",
  "dependencies": {},
  "devDependencies": {}
}
